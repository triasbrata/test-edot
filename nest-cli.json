{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/test-edot/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/test-edot/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/test-edot",
  "projects": {
    "test-edot": {
      "type": "application",
      "root": "apps/test-edot",
      "entryFile": "main",
      "sourceRoot": "apps/test-edot/src",
      "compilerOptions": {
        "tsConfigPath": "apps/test-edot/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "warehouse-service": {
      "type": "application",
      "root": "apps/warehouse-service",
      "entryFile": "main",
      "sourceRoot": "apps/warehouse-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/warehouse-service/tsconfig.app.json"
      }
    },
    "order-service": {
      "type": "application",
      "root": "apps/order-service",
      "entryFile": "main",
      "sourceRoot": "apps/order-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-service/tsconfig.app.json"
      }
    },
    "product-service": {
      "type": "application",
      "root": "apps/product-service",
      "entryFile": "main",
      "sourceRoot": "apps/product-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product-service/tsconfig.app.json"
      }
    },
    "shop-service": {
      "type": "application",
      "root": "apps/shop-service",
      "entryFile": "main",
      "sourceRoot": "apps/shop-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/shop-service/tsconfig.app.json"
      }
    }
  }
}